{
  "name": "mony-app",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22.14.0",
    "npm": ">=10.9.2"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "mobile:dev": "npm run dev -w mobile",
    "api:dev": "npm run dev -w api"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "turbo": "latest"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
